<script>jQuery(function ($) {

var comparerHandler = function() {
    $.post($(this).attr('href'), function (h) {
        $('a.compare').replaceWith($(h).click(comparerHandler));
        showSuccess(
            h.match('mode=del') ? 
            'Véhicule ajouté au <a target="_blank" href="/fr/neuf/comparateur"><u>comparateur</u></a>.' : 
            'Véhicule supprimé du <a target="_blank" href="/fr/neuf/comparateur"><u>comparateur</u></a>.'
        );
        if ($('.search-menu .compare-button .number').length) {
            if ($(h).data('count')) {
                $('.search-menu .compare-button .number').text($(h).data('count'));
            } else {
                $('.search-menu .compare-button .number').remove();
            }
        } else {
            $('.search-menu .compare-button a').html('<span class="number">' + $(h).data('count') + '</span>');
        }
    });
    return false;
};

$('a.compare').on('click', comparerHandler);

